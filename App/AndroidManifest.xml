<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.sanjeeviraj.bookfiletracking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScanActivity"
            android:label="Scan mode"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Add_Activity" />
        <activity
            android:name=".BookAddActivity"
            android:label="@string/title_activity_add"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.MainActivity" />
        </activity>
        <activity
            android:name=".ModifyActivity"
            android:label="@string/title_activity_modify"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.MainActivity" />
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.MainActivity" />
        </activity>
        <activity
            android:name=".ArrangeActivity"
            android:label="@string/title_activity_arrange"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.MainActivity" />
        </activity>
        <activity
            android:name=".AddBookDetailsActivity"
            android:label="@string/title_activity_add_details"
            android:parentActivityName=".BookAddActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BookAddActivity" />
        </activity>
        <activity
            android:name=".RackAddActivity"
            android:label="@string/title_activity_rack_add"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.MainActivity" />
        </activity>
        <activity
            android:name=".ModifyBookID"
            android:label="@string/title_activity_modify_book_id"
            android:parentActivityName=".ModifyActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.ModifyActivity" />
        </activity>
        <activity
            android:name=".ArrangeRackDetails"
            android:label="@string/title_activity_arrange_rack_details"
            android:parentActivityName=".ArrangeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.apps.sanjeeviraj.bookfiletracking.ArrangeActivity" />
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
